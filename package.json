{
  "name": "presentation-facets",
  "version": "0.3.0",
  "description": "The Augmented.js Next - Presentation Facets Module.",
  "main": "dist/presentation-facets.js",
  "scripts": {
    "dev": "webpack --mode development --module-bind js=babel-loader --progress",
    "build": "webpack --mode production --module-bind js=babel-loader --progress",
    "lint": "eslint ./dist/presentation-facets.js",
    "test": "mocha --require babel-core/register --require test/helper.js -c test/*Spec.js",
    "start": "node index.js",
    "docs": "rm -rf jsdoc && jsdoc ./src -r -d ./jsdoc/ -R ./README.md -t ./node_modules/foodoc/template -c ./jsdoc.conf.json -P ./package.json",
    "readme": "./node_modules/documentation/bin/documentation.js readme ./src/index.js -f md --section=API",
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "rebuild": "npm run clean && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Augmentedjs/presentation-facets.git"
  },
  "keywords": [
    "augmentedjs",
    "presentation"
  ],
  "author": "Bob Warren <jedi.master@augmentedjs.com> (http://www.augmentedjs.com)",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/polyfill": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/register": "^7.4.0",
    "acorn": "^6.1.1",
    "acorn-dynamic-import": "^4.0.0",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^2.0.1",
    "css-loader": "^2.1.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.3.0",
    "documentation": "^9.3.1",
    "eslint": "^5.16.0",
    "foodoc": "0.0.9",
    "jsdoc": "^3.5.5",
    "jsdom": "^14.0.0",
    "mocha": "^6.0.2",
    "mocha-jsdom": "^2.0.0"
  },
  "dependencies": {
    "presentation-decorator": "^2.6.0",
    "presentation-dom": "^2.8.1"
  },
  "bugs": {
    "url": "https://github.com/Augmentedjs/presentation-facets/issues"
  },
  "homepage": "http://www.augmentedjs.com"
}
